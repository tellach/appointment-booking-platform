{
  "name": "medical-appointment-booking-platform",
  "version": "1.0.0",
  "description": "A platform for appointment booking, designed for doctors and medical offices",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.5.1",
    "electron-rebuild": "^1.10.1",
    "sqlite3": "^4.1.1"
  },
  "dependencies": {
    "jquery": "^3.5.0",
    "jquery-timepicker": "^1.3.3",
    "mysql2": "^2.1.0",
    "save": "^2.4.0",
    "sequelize": "^5.21.6",
    "sqlite": "^4.0.6"
  },
  "build": {
    "appId": "com.appointment-booking.app",
    "mac": {
      "category": "com.appointment-booking.app"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "npmRebuild": true
